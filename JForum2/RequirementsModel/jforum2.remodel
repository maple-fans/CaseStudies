	Contract JForum2System::login(userId : String, password : String) : Boolean {

		definition:
			user:User = User.allInstance()->any(u:User | u.UserId = userId and u.Password = password)

		precondition:
			user.oclIsUndefined() = false
		postcondition:
			LoginUser = user and
			result = true
	}

	Contract BrowseService::listComment(postId : String) : Sequence(Comment) {

		definition:
			post:Post = Post.allInstance()->any(p:Post | p.PostId = postId)

		precondition:
			post.oclIsUndefined() = false
		postcondition:
			result = post.Comments
	}

	Contract BrowseService::listPost(topicId : String) : Sequence(Post) {

		definition:
			topic:Topic = Topic.allInstance()->any(t:Topic|t.TopicId = topicId)

		precondition:
			topic.oclIsUndefined() = false
		postcondition:
			result = topic.Posts
	}

	Contract BrowseService::listTopic(forumId : String) : Sequence(Topic) {

		definition:
			forum:Forum = Forum.allInstance()->any(f:Forum | f.ForumId = forumId)

		precondition:
			forum.oclIsUndefined() = false
		postcondition:
			result = forum.Topics
	}

	Contract BrowseService::listForum(categoryId : String) : Sequence(Forum) {

		definition:
			category:Category = Category.allInstance()->any(c:Category|c.CategoryId = categoryId)

		precondition:
			category.oclIsUndefined() = false
		postcondition:
			result = category.Forums
	}

	Contract BrowseService::listCategoty() : Sequence(Category) {

		precondition:
			true
		postcondition:
			result = Category.allInstance()
	}

	Contract UserManagementService::deleteUser(userId : String) : Boolean {

		definition:
			user:User = User.allInstance()->any(u:User| u.UserId = userId)

		precondition:
			user.oclIsUndefined() = false
		postcondition:
			User.allInstance()->excludes(user) and
			result = true
	}

	Contract UserManagementService::updateUser(userId : String, userName : String, password : String, email : String) : Boolean {

		definition:
			user:User = User.allInstance()->any(u:User| u.UserId = userId)

		precondition:
			user.oclIsUndefined() = false
		postcondition:
			user.UserName = userName and
			user.Password = password and
			user.Email = email and
			result = true
	}

	Contract UserManagementService::findUser(userId : String) : User {

		definition:
			user:User = User.allInstance()->any(u:User| u.UserId = userId)

		precondition:
			user.oclIsUndefined() = false
		postcondition:
			result = user
	}

	Contract UserManagementService::listAllUser() : Sequence(User) {

		precondition:
			true
		postcondition:
			result = User.allInstance()
	}

	Contract UserManagementService::addUser(userId : String, userName : String, password : String, email : String, registrationDate : Date) : Boolean {

		definition:
			user:User = User.allInstance()->any(u:User| u.UserId = userId)

		precondition:
			user.oclIsUndefined() = true
		postcondition:
			let u:User in
			u.oclIsNew() and
			u.UserId = userId and
			u.UserName = userName and
			u.Password = password and
			u.Email = email and
			u.RegistrationDate = registrationDate and
			User.allInstance()->includes(u) and
			result = true
	}

