DomainModel JForum2 {

	Entity User {
		UserId : String
		UserName : String
		Password : String
		Email : String
		RegistrationDate : Date
	}

	Entity Forum {
		ForumId : String
		ForumName : String
		Description : String

		[Refer]
		Topics : Topic* Association
	}

	Entity Post {
		PostId : String
		Subject : String
		Content : String
		CreatedDate : Date

		[Refer]
		User: User Association
		Comments: Comment* Association
	}

	Entity Topic {
		TopicId : String
		TopicName : String
		CreatedDate : Date

		[Refer]
		Posts : Post* Association
	}

	Entity Category {
		CategoryId : String
		CategoryName : String

		[Refer]
		Forums : Forum* Association
	}

	Entity Comment {
		CommentId : String
		Content : String
		CreatedDate : Date

		[Refer]
		User: User Association
	}

}

